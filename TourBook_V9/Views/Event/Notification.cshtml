<!-- header , CSS, Jscripts for navigation-->

<style>
    .img {
       background-image: url('../../img/img4.jpg');
       
    }
    .nav-item > .nav-link {
        color: black !important;
    }

    .logo {
        color: #000 !important;
        font-weight: 600 !important;
    }

    .content-banner .first-title {
        font-size: 30px !important;
    }

    }

    .header {
        position: relative;
        width: 100%;
        opacity:1!important
    }

    .header-inner {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        z-index: 99;
        background-color: transparent;
    }

    .logo {
        color: #ffffff;
    }

    .nav-item .nav-link {
        display: block;
        line-height: 36px;
        text-transform: capitalize;
        font-size: 16px;
        font-weight: 500;
        color: hsl(0, 88%, 67%);
        transition: 0.15s;
    }

        .nav-item .nav-link:hover {
            color: #094b2a;
        }

    .header-btn {
        color: white;
        border-radius: 30px;
        background-color: gray;
        border: none;
        font-weight: 500;
        outline: none;
        font-size: 15px;
        padding: 7px 22px;
        transition: 0.5s;
    }

        .header-btn:hover {
            background-color: #fe4066;
            cursor: pointer;
        }

    .navbar-scroll {
        background-color: #ffffff;
        padding: 0;
        position: fixed;
        top: 0;
        z-index: 99;
        box-shadow: 0px 1px 10px rgba(0, 0, 0, 0.4);
        transition-duration: 0.6s;
    }

        .navbar-scroll .nav-item .nav-link {
            color: black;
        }

        .navbar-scroll .header-btn {
            color: #0c0b0b;
            background-color: #fe4066;
        }

        .navbar-scroll .logo {
            color: #000;
            font-weight: 500;
        }

    .first-title {
        text-transform: capitalize;
    }

    section {
        background-color: white;
        padding: 4rem 0rem 4rem;
    }

        section:nth-of-type(even) {
            background-color: #fe4066;
            color: white;
        }
</style>



